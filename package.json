{
  "name": "@fss/components",
  "version": "1.2.1",
  "description": "",
  "keywords": [],
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib",
    "styles",
    "types"
  ],
  "author": "Paul Sachs <paul.sachs@ibm.com>",
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/wfss-finance/wfss-components.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "build": "gulp build",
    "start": "start-storybook -p 6007",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "prepublish": "yarn build",
    "publish:storybook": "build-storybook && gh-pages -d storybook-static"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "./test-setup/setup.ts",
    "moduleNameMapper": {
      "@fss/components": "<rootDir>/src",
      "\\.(css|jpg|png)$": "<rootDir>/test-setup/empty-module.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.3",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/preset-react": "^7.0.0-rc.3",
    "@babel/preset-stage-0": "^7.0.0-rc.3",
    "@babel/preset-typescript": "^7.0.0-rc.3",
    "@fss/icons": "^1.2.10",
    "@storybook/addon-actions": "4.0.0-alpha.16",
    "@storybook/addon-info": "4.0.0-alpha.16",
    "@storybook/addon-links": "4.0.0-alpha.16",
    "@storybook/addon-options": "4.0.0-alpha.16",
    "@storybook/addon-storyshots": "4.0.0-alpha.16",
    "@storybook/addon-storysource": "4.0.0-alpha.16",
    "@storybook/addons": "4.0.0-alpha.16",
    "@storybook/react": "4.0.0-alpha.16",
    "@types/classnames": "^2.2.3",
    "@types/jest": "^22.0.0",
    "@types/node": "^9.3.0",
    "@types/ramda": "^0.25.33",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/storybook__react": "^3.0.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-plugin-emotion": "^9.2.6",
    "carbon-components": "^8.10.0",
    "carbon-components-react": "^5.43.1",
    "carbon-icons": "^6.0.0",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "css-gridish": "^2.0.0",
