{
  "name": "@fss/components",
  "version": "0.4.0",
  "description": "",
  "keywords": [],
  "main": "wfss-components.umd.js",
  "module": "wfss-components.es5.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib",
    "styles",
    "types",
    "wfss-components.umd.js",
    "wfss-components.es5.js"
  ],
  "author": "Paul Sachs <paul.sachs@ibm.com>",
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/wfss-finance/wfss-components.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "build": "gulp build && rollup -c rollup.config.ts",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "prepublish": "yarn build",
    "publish:storybook": "build-storybook && gh-pages -d storybook-static"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "./test-setup/setup.ts",
    "moduleNameMapper": {
      "@fss/components": "<rootDir>/src",
      "\\.(css|jpg|png)$": "<rootDir>/test-setup/empty-module.js"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-info": "^3.4.6",
    "@storybook/addon-links": "^3.4.6",
    "@storybook/addon-options": "^3.4.7",
    "@storybook/addon-storyshots": "^3.4.6",
    "@storybook/addon-storysource": "^3.4.6",
    "@storybook/addons": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "@types/classnames": "^2.2.3",
    "@types/jest": "^22.0.0",
    "@types/node": "^9.3.0",
    "@types/ramda": "^0.25.33",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/storybook__react": "^3.0.7",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "carbon-components": "^8.10.0",
    "carbon-components-react": "^5.43.1",
    "carbon-icons": "^6.0.0",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "css-gridish": "^2.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "del": "^3.0.0",
    "emotion": "^9.1.3",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-typescript": "^5.0.0-alpha.2",
    "husky": "^0.14.0",
    "jest": "^22.0.2",
    "jest-emotion": "^9.2.2",
    "lint-staged": "^7.0.0",
    "lodash.camelcase": "^4.3.0",
    "merge2": "^1.2.2",
    "prettier": "^1.13.5",
    "prompt": "^1.0.0",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-docgen-typescript-loader": "^2.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.2.1",
    "react-test-renderer": "^16.4.0",
    "replace-in-file": "^3.0.0-beta.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.57.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.11.1",
    "storybook-readme": "^3.3.0",
    "ts-jest": "^22.0.0",
    "ts-loader": "3.x",
    "ts-node": "^5.0.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.11.0",
    "typescript": "^2.9.1",
    "validate-commit-msg": "^2.12.2"
  },
  "peerDependencies": {
    "carbon-components-react": "5.x",
    "emotion": "9.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "publishConfig": {
    "registry": "https://na.artifactory.swg-devops.com:443/artifactory/api/npm/ip-wfss-npm-local"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
}
