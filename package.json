    "ts-jest": "^22.0.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.11.0",
    "typescript": "^3.0.3",
    "validate-commit-msg": "^2.12.2",
    "webpack": "^4.16.5"
  },
  "peerDependencies": {
    "carbon-components-react": "*",
    "emotion": "9.x",
    "emotion-theming": "9.x",
    "react": ">=16.4",
    "react-dom": ">=16.4",
    "react-emotion": "9.x"
  },
  "publishConfig": {
